<script type="text/template" id="ListTemplate" class="none">
	<div class="tableCont" data-coperatecd="{{COPORATE_CD}}" data-plantcd="{{PLANT_CD}}" data-moveno="{{MOVE_NO}}" data-movetype="{{MOVE_TYPE}}" data-movegb="{{MOVE_GB}}" data-movedesc="{{MOVE_DESC}}" data-moveseq="{{MOVE_SEQ}}" data-partcd="{{PART_CD}}" data-moveqty="{{MOVE_QTY}}" data-vendorcd="{{VENDOR_CD}}" data-vendornm="{{VENDOR_NM}}" data-moveyn="{{MOVE_YN}}">
		<div>
			<p class="PART_CD">{{PART_CD}}</p>
		</div>
		<div>
			<p class="MOVE_QTY">{{MOVE_QTY}}</p>
		</div>
	</div>
</script>
