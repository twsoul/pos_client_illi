							</div>
							<div class="opt_input" style="margin-top:2px;"><!--반출번호-->
								<p class="lineHead" data-lng="LB.0000000182"></p><p id="txtORDR_NO"></p>
							</div>
							<div class="list_area adjForm" style="margin-top:2px;">
								<table class="table_area">
									<colgroup>
										<col style="width:50%;">
										<col style="width:50%;">
									</colgroup>
									<thead>
										<tr>
											<th><span data-lng="LB.0000000039"></span></th><!--입고일자-->
											<th><span data-lng="LB.0000000427"></span></th><!--TM-->
										</tr>
									</thead>
									<tbody>
										<tr>
											<td><span id="txtINDATE"></span></td>
											<td align="center"><span id="txtTMTAG" class="nRedbox1">0</span></td>
										</tr>
									</tbody>
								</table>
							</div>
						</div>
						<div class="moreArea expand"><p><i></i></p></div>
					</article>
				</div>
			</section>

			<section>
				<div class="container">
					<article class="tableList">
						<div class="list_inner list_pd_071 blind" id="list_pd_071_head">
							<div class="tableHead">
								<div>
									<p data-lng="LB.0000000038"></p><!--품번-->
								</div>
								<div>
									<p data-lng="LB.0000000149"></p><!--지시수량-->
								</div>
							</div>
						</div>
						<div class="list_inner list_pd_071 adjFix" id="list_pd_071">
						</div>
					</article>
				</div>
			</section>
		</main>
	</div>

	<!-- footer -->
