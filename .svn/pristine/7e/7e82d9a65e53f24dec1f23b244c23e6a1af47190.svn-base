/*---------「 reset 」--------*/
* {padding:0; margin:0; vertical-align:baseline; line-height:1; font-family: "HDharmonyL"; letter-spacing: -0.025em;}

body {-webkit-text-size-adjust:none; -ms-text-size-adjust:none; background:#fff;}
h1, h2, h3, h4, h5, h6, strong, thead, th {font-weight:normal;}
ul, ol, li {list-style:none;}
table, thead, tbody, tr, td, th {vertical-align:middle; border-collapse:collapse; border-spacing:0}
input, button, select {width:100%; height:100%; appearance:none; -moz-appearance: none; -webkit-appearance: none; border:none; outline:none; background:transparent; cursor:pointer;}
input:focus, select:focus, textarea, textarea:focus, option {border:none; outline:none;}
code, kbd, pre, samp { font-family: monospace, monospace; font-size: 1rem; }
textarea {resize:none;  outline:none; background:none; border:none;}
img {max-height:100%;}
em, i {font-style:normal;}
option {font-size:1rem;}
a, a:hover {color:#333; vertical-align:middle;}
/* clearfix */
.clearfix {*zoom:1;} 
.clearfix:before, .clearfix:after {content:"";  display:block; line-height:0; }
.clearfix:after{clear:both;}
/* placeholder */
input::-webkit-input-placeholder, textarea::-webkit-input-placeholder {color:#aaa; } 
input:-moz-placeholder, textarea:-moz-placeholder {color:#aaa; } 
input::-moz-placeholder, textarea::-moz-placeholder {color:#aaa; } 
input:-ms-input-placeholder, textarea:-ms-input-placeholder {color:#aaa; }
input:focus::-webkit-input-placeholder, textarea:focus::-webkit-input-placeholder {color:transparent; } 
input:focus:-moz-placeholder, textarea:focus:-moz-placeholder {color:transparent; } 
input:focus::-moz-placeholder, textarea:focus::-moz-placeholder {color:transparent; } 
input:focus:-ms-input-placeholder, textarea:focus:-ms-input-placeholder {color:transparent; }

/*---------「 consisly code 」--------*/
.container {max-width:100%; box-sizing:border-box; padding:0 4.6%; }
.blocked {display:block; width:100%; height: =100%;} /* only button, select, input */
.blind {display:none !important; width:0 !important; height:0 !important; font-size:0 !important; line-height:0; position: absolute; z-index: -1; visibility:hidden;}
.fullscreen {height:100vmax;}
.non_headCont {padding-top:2.6862vmax;}
.under_headCont {padding-top:0.5vmax;}
.link_call {text-decoration:underline;}
.non {opacity:0; visibility:hidden;}
.tum {display:inline-block; padding:0 2.2%; box-sizing:border-box;}
.adjFix {padding-bottom:19rem;}
.mb2 {margin-bottom:2vmin;}
.adjArray {text-align:initial; box-sizing:border-box; padding:0 1.4%;}
.rollingPossible {overflow-y:scroll;height:250px !important;}
.rollingPossible > div:nth-child(1) {border-top:none;}
.cutText {white-space:nowrap; text-overflow:ellipsis; overflow:hidden; box-sizing:border-box; padding:0 1.5%;}

/*---------「 popup 」--------*/
.Pop_alert_area {position:absolute; top:0; bottom: 0; left:0; right:0; background:rgba(0,0,0,0.8);}
.pop_contents {position:absolute; top:0; bottom: 0; eft:0; right:0; margin:auto; width:50%; height:50%; border:1px solid #fff;}
